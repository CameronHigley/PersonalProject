@model IList<ApplicationUser>

<h1>User Accounts:</h1>
<table>
    <thead>
        <th>UserName</th>
        <th></th>
    </thead>
    <tbody>
        @foreach(ApplicationUser user in @Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td><a asp-controller="Admin" asp-action="DeleteUser" asp-route-id="@user.Id" class="btn btn-primary">Delete</a></td>
            </tr>
            
        }
    </tbody>
</table>